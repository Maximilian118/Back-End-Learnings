    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.1",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
