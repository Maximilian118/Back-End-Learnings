{
  "name": "backend-learnings",
  "version": "1.0.0",
  "description": "To facilitate my learning of Backend languages and practices",
  "main": "index.js",
  "scripts": {
    "node-server": "nodemon ./node/src-node/js/index",
    "express-server": "nodemon ./node/express/src-express/js/index",
    "api-server": "nodemon ./node/api-node/server",
    "graphql-backend": "nodemon ./graphql/backend/app.js",
    "graphql-frontend": "npm run start --prefix ./graphql/frontend",
    "graphql-dev": "concurrently \"npm run graphql-backend\" \"npm run graphql-frontend\"",
    "bundler": "parcel ./node/src-node/index.html",
    "twitbot": "node ./node/twitter-bot/twitter-bot",
    "twitbotstream": "node ./node/twitter-bot/twitter-bot-stream"
  },
